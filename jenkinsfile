pipeline{

    agent  any

    stages{

        stage('Continues Download'){

            steps{
                git branch: 'main', url: 'https://github.com/STARGATELIGHT/loggin-app.git'
            }
        }
        stage('UNIT TEST'){

            steps{
                sh 'mvn test'
            }
        }
        stage('INTEGRETION TEST'){
            steps{
                sh 'mvn verify -DskipUnitTests'
            }
        }
    }
}